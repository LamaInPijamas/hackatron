<p style="color: green"><%= notice %></p>

<% content_for :title, "Data uploads" %>

<h1>Data uploads</h1>

<div id="data_uploads">
  <% @data_uploads.each do |data_upload| %>
    <%= render data_upload %>
    <p>
      <%= link_to "Show this data upload", data_upload %>
    </p>
  <% end %>
</div>

<%= link_to "New data upload", new_data_upload_path %>
